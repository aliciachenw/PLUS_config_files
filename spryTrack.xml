        <!-- <DataSource Type="Tool" Id="Reference" TrackingType="ACTIVE" GeometryId="371000" /> -->
      </DataSources>
      <OutputChannels>
        <OutputChannel Id="TrackerStream">
        <!-- <DataSource Type="Tool" Id="Stylus" /> -->
        <!-- <DataSource Type="Tool" Id="Reference" /> -->
        </OutputChannel>
      </OutputChannels>
    </Device>
  </DataCollection>
  
</PlusConfiguration>